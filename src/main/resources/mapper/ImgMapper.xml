<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Img">
    <select id="findAll" resultType="com.demo.entity.Img">
        select *
        from img limit 10
    </select>
    <select id="findOne" resultType="com.demo.entity.Img">
        select *
        from img
        where id = 1000
    </select>

</mapper>